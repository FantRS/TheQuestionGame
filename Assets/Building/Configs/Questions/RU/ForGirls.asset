    \u0438\u043B\u0438 \u0441\u0435\u043C\u0435\u0439\u043D\u044B\u0439 \u043E\u0431\u044B\u0447\u0430\u0439
    \u0442\u0435\u0431\u0435 \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E \u0434\u043E\u0440\u043E\u0433?"
  - "\u0415\u0441\u043B\u0438 \u0431\u044B \u0442\u0435\u0431\u0435 \u043F\u0440\u0438\u0448\u043B\u043E\u0441\u044C
