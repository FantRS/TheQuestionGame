  - "\u042F\u043A\u0443 \u043F\u043E\u0440\u0430\u0434\u0443 \u0442\u0438 \u0434\u0430\u0432
    \u0431\u0438 \u0441\u043E\u0431\u0456 \u043F'\u044F\u0442\u0438\u0440\u0456\u0447\u043D\u043E\u0457
    \u0434\u0430\u0432\u043D\u043E\u0441\u0442\u0456?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043C\u0438\u0442\u0442\u0454\u0432\u043E
    \u0441\u0442\u0430\u0442\u0438 \u043C\u0430\u0439\u0441\u0442\u0440\u043E\u043C
    \u0443 \u0447\u043E\u043C\u0443\u0441\u044C, \u0449\u043E \u0431 \u0446\u0435
    \u0431\u0443\u043B\u043E?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438
    \u0434\u0435\u043D\u044C \u0456\u0437 \u0443\u043B\u044E\u0431\u043B\u0435\u043D\u0438\u043C
    \u0441\u043F\u043E\u0440\u0442\u0441\u043C\u0435\u043D\u043E\u043C, \u0445\u0442\u043E
    \u0431 \u0446\u0435 \u0431\u0443\u0432?"
  - "\u042F\u043A\u0456 \u043D\u0430\u0432\u0438\u0447\u043A\u0438 \u0442\u0438 \u0445\u043E\u0442\u0456\u0432
    \u0431\u0438 \u043E\u043F\u0430\u043D\u0443\u0432\u0430\u0442\u0438 \u0432 \u043C\u0430\u0439\u0431\u0443\u0442\u043D\u044C\u043E\u043C\u0443?"
  - "\u0422\u0438 \u0441\u0442\u0440\u0438\u0431\u043D\u0443\u0432 \u0431\u0438 \u0437
    \u043F\u0430\u0440\u0430\u0448\u0443\u0442\u043E\u043C \u0430\u0431\u043E \u0441\u043F\u0440\u043E\u0431\u0443\u0432\u0430\u0432
    \u0456\u043D\u0448\u0438\u0439 \u0435\u043A\u0441\u0442\u0440\u0435\u043C\u0430\u043B\u044C\u043D\u0438\u0439
    \u0441\u043F\u043E\u0440\u0442?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u0431\u0443\u0432 \u0433\u0435\u0440\u043E\u0454\u043C
    \u0431\u043E\u0439\u043E\u0432\u0438\u043A\u0430, \u0445\u0442\u043E \u0431 \u0441\u0442\u0430\u0432
    \u0442\u0432\u043E\u0457\u043C \u043D\u0430\u043F\u0430\u0440\u043D\u0438\u043A\u043E\u043C?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u0441\u0442\u0430\u0432 \u0441\u0443\u043F\u0435\u0440\u0433\u0435\u0440\u043E\u0454\u043C,
    \u044F\u043A\u0430 \u0431 \u0443 \u0442\u0435\u0431\u0435 \u0431\u0443\u043B\u0430
    \u0441\u0443\u043F\u0435\u0440\u0441\u0438\u043B\u0430?"
  - "\u042F\u043A \u0431\u0438 \u043D\u0430\u0437\u0438\u0432\u0430\u0432\u0441\u044F
    \u0444\u0456\u043B\u044C\u043C \u043F\u0440\u043E \u0442\u0432\u043E\u0454 \u0436\u0438\u0442\u0442\u044F?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u0457\u0441\u0442\u0438
    \u043B\u0438\u0448\u0435 \u043E\u0434\u043D\u0443 \u0457\u0436\u0443 \u0434\u043E
    \u043A\u0456\u043D\u0446\u044F \u0436\u0438\u0442\u0442\u044F, \u0449\u043E \u0431
    \u0446\u0435 \u0431\u0443\u043B\u043E?"
  - "\u042F\u043A\u0438\u0439 \u0434\u0438\u0432\u043D\u0438\u0439 \u0441\u043E\u043D
    \u0442\u043E\u0431\u0456 \u0441\u043D\u0438\u0432\u0441\u044F?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u0432\u0438\u0433\u0440\u0430\u0432 \u043C\u0456\u043B\u044C\u0439\u043E\u043D
    \u0434\u043E\u043B\u0430\u0440\u0456\u0432, \u0449\u043E \u0431 \u0442\u0438
    \u0437\u0440\u043E\u0431\u0438\u0432 \u043D\u0430\u0441\u0430\u043C\u043F\u0435\u0440\u0435\u0434?"
  - "\u041A\u0438\u043C \u0442\u0438 \u043C\u0440\u0456\u044F\u0432 \u0441\u0442\u0430\u0442\u0438
    \u0443 \u0434\u0438\u0442\u0438\u043D\u0441\u0442\u0432\u0456?"
  - "\u042F\u043A\u0438\u0439 \u0436\u0430\u0440\u0442 \u0437\u0430\u0432\u0436\u0434\u0438
    \u0437\u043C\u0443\u0448\u0443\u0454 \u0442\u0435\u0431\u0435 \u0441\u043C\u0456\u044F\u0442\u0438\u0441\u044F?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0430\u0432 \u0442\u0440\u0438
    \u0431\u0430\u0436\u0430\u043D\u043D\u044F, \u0449\u043E \u0431 \u0442\u0438
    \u0437\u0430\u0433\u0430\u0434\u0430\u0432?"
  - "\u042F\u043A\u0438\u0439 \u043C\u0456\u0444\u0456\u0447\u043D\u0438\u0439 \u0447\u0438
    \u0432\u0438\u0433\u0430\u0434\u0430\u043D\u0438\u0439 \u0441\u0432\u0456\u0442
    \u0442\u0438 \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u0432\u0456\u0434\u0432\u0456\u0434\u0430\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u043E\u0431\u0456 \u0434\u043E\u0432\u0435\u043B\u043E\u0441\u044F
    \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438 \u043C\u0456\u0441\u044F\u0446\u044C
    \u043D\u0430 \u0431\u0435\u0437\u043B\u044E\u0434\u043D\u043E\u043C\u0443 \u043E\u0441\u0442\u0440\u043E\u0432\u0456,
    \u0449\u043E \u0431 \u0442\u0438 \u0432\u0437\u044F\u0432 \u0456\u0437 \u0441\u043E\u0431\u043E\u044E?"
  - "\u0422\u0438 \u043C\u0456\u0433 \u0431\u0438 \u043F\u0440\u043E\u0436\u0438\u0442\u0438
    \u0442\u0438\u0436\u0434\u0435\u043D\u044C \u0431\u0435\u0437 \u0442\u0435\u043B\u0435\u0444\u043E\u043D\u0443?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0430\u0432 \u043E\u0431\u0440\u0430\u0442\u0438
    \u043B\u0438\u0448\u0435 \u043E\u0434\u0438\u043D \u0432\u0438\u0434 \u0442\u0440\u0430\u043D\u0441\u043F\u043E\u0440\u0442\u0443,
    \u0449\u043E \u0431 \u0446\u0435 \u0431\u0443\u043B\u043E?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u0441\u0442\u0430\u0442\u0438
    \u043D\u0435\u0432\u0438\u0434\u0438\u043C\u0438\u043C \u043D\u0430 \u043E\u0434\u0438\u043D
    \u0434\u0435\u043D\u044C, \u0449\u043E \u0442\u0438 \u0437\u0440\u043E\u0431\u0438\u0432
    \u0431\u0438?"
  - "\u042F\u043A\u0438\u0439 \u0441\u0430\u043C\u0438\u0439 \u043D\u0435\u0437\u0432\u0438\u0447\u0430\u0439\u043D\u0438\u0439
    \u0433\u0430\u0434\u0436\u0435\u0442 \u0442\u0438 \u0445\u043E\u0442\u0456\u0432
    \u0431\u0438 \u043C\u0430\u0442\u0438?"
  - "\u0429\u043E \u043A\u0440\u0430\u0449\u0435 \u2013 \u0441\u043F\u043E\u043A\u0456\u0439\u043D\u0438\u0439
    \u0432\u0435\u0447\u0456\u0440 \u0456\u0437 \u0434\u0440\u0443\u0437\u044F\u043C\u0438
    \u0447\u0438 \u0433\u0430\u043B\u0430\u0441\u043B\u0438\u0432\u0430 \u0432\u0435\u0447\u0456\u0440\u043A\u0430?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u0441\u0442\u0430\u0442\u0438
    \u043D\u0430\u0439\u043A\u0440\u0430\u0449\u0438\u043C \u0443 \u044F\u043A\u0456\u0439\u0441\u044C
    \u043F\u0440\u043E\u0444\u0435\u0441\u0456\u0457, \u0449\u043E \u0431 \u0446\u0435
    \u0431\u0443\u043B\u043E?"
  - "\u0422\u0438 \u0437\u043C\u0456\u0433 \u0431\u0438 \u0432\u0438\u0436\u0438\u0442\u0438
    \u0432 \u0440\u0435\u0430\u043B\u044C\u043D\u043E\u043C\u0443 \u043A\u0432\u0435\u0441\u0442\u0456
    \u043D\u0430 \u0432\u0438\u0436\u0438\u0432\u0430\u043D\u043D\u044F?"
  - "\u042F\u043A\u0443 \u0441\u0443\u043F\u0435\u0440\u0437\u0434\u0430\u0442\u043D\u0456\u0441\u0442\u044C
    \u0442\u0438 \u0432\u0438\u0431\u0440\u0430\u0432 \u0431\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u0431\u0443\u0432 \u0443 \u043C\u0438\u043D\u0443\u043B\u043E\u043C\u0443,
    \u0449\u043E \u0431 \u0442\u0438 \u0437\u043C\u0456\u043D\u0438\u0432?"
  - "\u0422\u0438 \u043F\u043E\u043B\u0435\u0442\u0456\u0432 \u0431\u0438 \u0432
    \u043A\u043E\u0441\u043C\u043E\u0441, \u044F\u043A\u0431\u0438 \u0431\u0443\u043B\u0430
    \u043C\u043E\u0436\u043B\u0438\u0432\u0456\u0441\u0442\u044C?"
  - "\u042F\u043A\u0438\u0439 \u0432\u0438\u043D\u0430\u0445\u0456\u0434 \u0442\u0438
    \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u0441\u0442\u0432\u043E\u0440\u0438\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043F\u043E\u0442\u0440\u0430\u043F\u0438\u0432
    \u0443 \u0433\u0440\u0443 \u043D\u0430 \u0432\u0438\u0436\u0438\u0432\u0430\u043D\u043D\u044F,
    \u044F\u043A \u0431\u0438 \u0442\u0438 \u0434\u0456\u044F\u0432?"
  - "\u042F\u043A\u0438\u0439 \u0441\u0432\u0456\u0442 \u2013 \u043F\u043E\u0441\u0442\u0430\u043F\u043E\u043A\u0430\u043B\u0456\u043F\u0441\u0438\u0441
    \u0447\u0438 \u0442\u0435\u0445\u043D\u043E\u043B\u043E\u0433\u0456\u0447\u043D\u0435
    \u043C\u0430\u0439\u0431\u0443\u0442\u043D\u0454 \u2013 \u0442\u043E\u0431\u0456
    \u0431\u043B\u0438\u0436\u0447\u0438\u0439?"
  - "\u042F\u043A\u043E\u044E \u0442\u0432\u0430\u0440\u0438\u043D\u043E\u044E \u0442\u0438
    \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u0431\u0443\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u043E\u0431\u0456 \u0437\u0430\u043F\u0440\u043E\u043F\u043E\u043D\u0443\u0432\u0430\u043B\u0438
    \u0440\u043E\u043B\u044C \u0443 \u0431\u043E\u0439\u043E\u0432\u0438\u043A\u0443,
    \u044F\u043A\u0438\u0439 \u0433\u0435\u0440\u043E\u0439 \u0442\u0438 \u0431\u0443\u0432
    \u0431\u0438?"
  - "\u042F\u043A\u0443 \u043A\u0440\u0430\u0457\u043D\u0443 \u0442\u0438 \u043C\u0440\u0456\u0454\u0448
    \u0432\u0456\u0434\u0432\u0456\u0434\u0430\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043F\u0440\u043E\u0436\u0438\u0442\u0438
    \u043E\u0434\u0438\u043D \u0434\u0435\u043D\u044C \u044F\u043A \u043A\u043E\u0440\u043E\u043B\u044C,
    \u0449\u043E \u0431 \u0442\u0438 \u0437\u0440\u043E\u0431\u0438\u0432?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0430\u0432 \u043E\u0441\u043E\u0431\u0438\u0441\u0442\u0438\u0439
    \u043E\u0441\u0442\u0440\u0456\u0432, \u0449\u043E \u0431 \u0442\u0438 \u043F\u043E\u0431\u0443\u0434\u0443\u0432\u0430\u0432
    \u043D\u0430 \u043D\u044C\u043E\u043C\u0443?"
  - "\u0422\u0438 \u0432\u0438\u0431\u0440\u0430\u0432 \u0431\u0438 \u0436\u0438\u0442\u0438
    \u0432 \u043C\u0430\u0439\u0431\u0443\u0442\u043D\u044C\u043E\u043C\u0443 \u0447\u0438
    \u0432 \u043C\u0438\u043D\u0443\u043B\u043E\u043C\u0443? \u0427\u043E\u043C\u0443?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438
    \u0432\u0435\u0447\u0456\u0440 \u0437 \u0431\u0443\u0434\u044C-\u044F\u043A\u043E\u044E
    \u043B\u044E\u0434\u0438\u043D\u043E\u044E \u0432 \u0456\u0441\u0442\u043E\u0440\u0456\u0457,
    \u0445\u0442\u043E \u0431 \u0446\u0435 \u0431\u0443\u0432?"
  - "\u0422\u0438 \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u0441\u0442\u0430\u0442\u0438
    \u0431\u0435\u0437\u0441\u043C\u0435\u0440\u0442\u043D\u0438\u043C? \u0427\u043E\u043C\u0443?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u0434\u0456\u0437\u043D\u0430\u0442\u0438\u0441\u044F,
    \u0449\u043E \u0441\u0442\u0430\u043D\u0435\u0442\u044C\u0441\u044F \u0447\u0435\u0440\u0435\u0437
    50 \u0440\u043E\u043A\u0456\u0432, \u0447\u0438 \u0441\u043A\u043E\u0440\u0438\u0441\u0442\u0430\u0432\u0441\u044F
    \u0431 \u0446\u0438\u043C \u0448\u0430\u043D\u0441\u043E\u043C?"
  - "\u042F\u043A\u0430 \u0442\u0432\u043E\u044F \u043D\u0430\u0439\u0434\u0438\u0432\u043D\u0456\u0448\u0430
    \u043C\u0440\u0456\u044F \u0447\u0438 \u0431\u0430\u0436\u0430\u043D\u043D\u044F?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0430\u0432 \u043E\u0441\u043E\u0431\u0438\u0441\u0442\u043E\u0433\u043E
    \u0440\u043E\u0431\u043E\u0442\u0430, \u0449\u043E \u0431 \u0442\u0438 \u0434\u043E\u0440\u0443\u0447\u0438\u0432
    \u0439\u043E\u043C\u0443 \u0440\u043E\u0431\u0438\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u0441\u0442\u0430\u0432 \u043F\u0440\u0435\u0437\u0438\u0434\u0435\u043D\u0442\u043E\u043C
    \u043D\u0430 \u0442\u0438\u0436\u0434\u0435\u043D\u044C, \u0449\u043E \u0431
    \u0442\u0438 \u0437\u043C\u0456\u043D\u0438\u0432?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043F\u043E\u0432\u0435\u0440\u043D\u0443\u0442\u0438\u0441\u044C
    \u0443 \u0431\u0443\u0434\u044C-\u044F\u043A\u0438\u0439 \u0447\u0430\u0441 \u0441\u0432\u043E\u0433\u043E
    \u0436\u0438\u0442\u0442\u044F, \u0449\u043E \u0431 \u0442\u0438 \u0437\u0440\u043E\u0431\u0438\u0432
    \u043F\u043E-\u0456\u043D\u0448\u043E\u043C\u0443?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433 \u043D\u0430\u043F\u0438\u0441\u0430\u0442\u0438
