    \u044F\u043A\u0443 \u0442\u0438 \u0432\u0438\u0431\u0440\u0430\u0432 \u0431\u0438?"
  - "\u0422\u0438 \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u0441\u0442\u0432\u043E\u0440\u0438\u0442\u0438
    \u0441\u0432\u043E\u044E \u0432\u0456\u0434\u0435\u043E\u0433\u0440\u0443? \u041F\u0440\u043E
    \u0449\u043E \u0431 \u0432\u043E\u043D\u0430 \u0431\u0443\u043B\u0430?"
  - "\u0422\u0438 \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u043F\u043E\u0431\u0443\u0432\u0430\u0442\u0438
    \u043D\u0430 \u0432\u0435\u0440\u0448\u0438\u043D\u0456 \u0415\u0432\u0435\u0440\u0435\u0441\u0442\u0443
    \u0447\u0438 \u0432 \u043D\u0430\u0439\u0433\u043B\u0438\u0431\u0448\u0456\u0439
    \u0442\u043E\u0447\u0446\u0456 \u043E\u043A\u0435\u0430\u043D\u0443?"
  - "\u0423\u044F\u0432\u0438, \u0449\u043E \u0442\u0438 \u043C\u043E\u0436\u0435\u0448
    \u0437\u0443\u043F\u0438\u043D\u044F\u0442\u0438 \u0447\u0430\u0441. \u0429\u043E
    \u0442\u0438 \u0431\u0443\u0434\u0435\u0448 \u0440\u043E\u0431\u0438\u0442\u0438?"
  - "\u0422\u0438 \u0445\u043E\u0442\u0456\u0432 \u0431\u0438 \u043F\u0440\u043E\u0436\u0438\u0442\u0438
    \u043E\u0434\u043D\u0435 \u0436\u0438\u0442\u0442\u044F \u0432 \u043D\u0435\u0441\u043A\u0456\u043D\u0447\u0435\u043D\u043D\u043E\u043C\u0443
    \u0449\u0430\u0441\u0442\u0456 \u0447\u0438 10 \u0440\u0456\u0437\u043D\u0438\u0445
    \u0436\u0438\u0442\u0442\u0456\u0432 \u0437 \u0440\u0456\u0437\u043D\u0438\u043C
    \u0434\u043E\u0441\u0432\u0456\u0434\u043E\u043C?"
  - "\u0422\u0438 \u043F\u043E\u0433\u043E\u0434\u0438\u0432\u0441\u044F \u0431 \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438
    \u0440\u0456\u043A \u0443 \u043F\u043E\u0432\u043D\u0456\u0439 \u0441\u0430\u043C\u043E\u0442\u0456,
    \u0449\u043E\u0431 \u0432\u0438\u043A\u043E\u043D\u0430\u0442\u0438 \u0441\u0432\u043E\u044E
    \u043D\u0430\u0439\u0431\u0456\u043B\u044C\u0448\u0443 \u043C\u0440\u0456\u044E?"
  - "\u042F\u043A\u0435 \u0434\u0438\u0432\u043E \u0441\u0432\u0456\u0442\u0443 \u0442\u0438
    \u0434\u043E\u0434\u0430\u0432 \u0431\u0438 \u0434\u043E \u043D\u0430\u0448\u043E\u0433\u043E
