  - "\u042F\u043A\u0431\u0438 \u043D\u0430\u0448\u0456 \u0434\u043E\u043C\u0430\u0448\u043D\u0456
    \u0442\u0432\u0430\u0440\u0438\u043D\u0438 \u043C\u043E\u0433\u043B\u0438 \u0433\u043E\u0432\u043E\u0440\u0438\u0442\u0438,
    \u0449\u043E \u0431 \u0432\u043E\u043D\u0438 \u0441\u043A\u0430\u0437\u0430\u043B\u0438
    \u043F\u0440\u043E \u043D\u0430\u0441?"
  - "\u0425\u0442\u043E \u0437 \u043D\u0430\u0441 \u0447\u0430\u0441\u0442\u0456\u0448\u0435
    \u0437\u0430 \u0432\u0441\u0456\u0445 \u043A\u0430\u0436\u0435, \u0449\u043E
    \u043C\u0430\u0454 \u0440\u0430\u0446\u0456\u044E, \u043D\u0430\u0432\u0456\u0442\u044C
    \u044F\u043A\u0449\u043E \u0446\u0435 \u043D\u0435 \u0442\u0430\u043A?"
  - "\u042F\u043A\u0431\u0438 \u043D\u0430\u0448\u0430 \u0441\u0456\u043C\u2019\u044F
    \u0431\u0440\u0430\u043B\u0430 \u0443\u0447\u0430\u0441\u0442\u044C \u0443 \u0433\u0440\u0456
    \xAB\u0425\u0442\u043E \u043D\u0430\u0439\u043A\u0440\u0430\u0449\u0438\u0439?\xBB,
    \u0443 \u0447\u043E\u043C\u0443 \u0431 \u043C\u0438 \u043F\u0435\u0440\u0435\u043C\u043E\u0433\u043B\u0438?"
  - "\u0425\u0442\u043E \u0437 \u043D\u0430\u0441 \u0447\u0430\u0441\u0442\u0456\u0448\u0435
    \u0437\u0430 \u0432\u0441\u0456\u0445 \u0437\u0430\u0431\u0443\u0432\u0430\u0454
    \u0432\u0438\u043A\u0438\u043D\u0443\u0442\u0438 \u0441\u043C\u0456\u0442\u0442\u044F?"
  - "\u0429\u043E \u0431 \u0442\u0438 \u0437\u0440\u043E\u0431\u0438\u0432(\u043B\u0430),
    \u044F\u043A\u0431\u0438 \u0440\u0430\u043F\u0442\u043E\u0432\u043E \u043E\u043F\u0438\u043D\u0438\u0432\u0441\u044F(\u043B\u0430\u0441\u044F)
    \u043D\u0430 \u0431\u0435\u0437\u043B\u044E\u0434\u043D\u043E\u043C\u0443 \u043E\u0441\u0442\u0440\u043E\u0432\u0456
    \u0437 \u043D\u0430\u0448\u043E\u044E \u0441\u0456\u043C\u2019\u0454\u044E?"
  - "\u042F\u043A\u0456 \u0441\u0443\u043F\u0435\u0440\u0437\u0434\u0456\u0431\u043D\u043E\u0441\u0442\u0456
    \u0442\u0438 \u0431 \u0445\u043E\u0442\u0456\u0432(\u043B\u0430), \u044F\u043A\u0431\u0438
    \u0446\u0435 \u0434\u043E\u043F\u043E\u043C\u043E\u0433\u043B\u043E \u0442\u043E\u0431\u0456
    \u0432 \u043F\u043E\u0431\u0443\u0442\u043E\u0432\u0438\u0445 \u0441\u043F\u0440\u0430\u0432\u0430\u0445?"
  - "\u042F\u043A \u0442\u0438 \u0434\u0443\u043C\u0430\u0454\u0448, \u0445\u0442\u043E
    \u0437 \u043D\u0430\u0441 \u043D\u0430\u0439\u043A\u0440\u0430\u0449\u0435 \u0432\u043F\u043E\u0440\u0430\u0432\u0441\u044F
    \u0431 \u0456\u0437 \u0436\u0438\u0442\u0442\u044F\u043C \u0443 \u043A\u0430\u043C\u2019\u044F\u043D\u043E\u043C\u0443
    \u0432\u0456\u0446\u0456?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433(\u043C\u043E\u0433\u043B\u0430)
    \u0437\u0430\u0433\u0430\u0434\u0430\u0442\u0438 \u043E\u0434\u043D\u0435 \u0431\u0430\u0436\u0430\u043D\u043D\u044F
    \u0434\u043B\u044F \u0432\u0441\u0456\u0454\u0457 \u0441\u0456\u043C\u2019\u0457,
    \u0449\u043E \u0431 \u0446\u0435 \u0431\u0443\u043B\u043E?"
  - "\u0429\u043E \u0431 \u0442\u0438 \u0437\u0440\u043E\u0431\u0438\u0432(\u043B\u0430),
    \u044F\u043A\u0431\u0438 \u0443 \u043D\u0430\u0441 \u0437\u2019\u044F\u0432\u0438\u0432\u0441\u044F
    \u043A\u043B\u043E\u043D \u043A\u043E\u0436\u043D\u043E\u0433\u043E \u0447\u043B\u0435\u043D\u0430
    \u0441\u0456\u043C\u2019\u0457?"
  - "\u042F\u043A\u0431\u0438 \u0442\u0438 \u043C\u0456\u0433(\u043C\u043E\u0433\u043B\u0430)
    \u0441\u0442\u0430\u0442\u0438 \u0443\u0447\u0430\u0441\u043D\u0438\u043A\u043E\u043C
    \xAB\u0413\u043E\u043B\u043E\u0434\u043D\u0438\u0445 \u0456\u0433\u043E\u0440\xBB,
    \u0437 \u043A\u0438\u043C \u0456\u0437 \u0441\u0456\u043C\u2019\u0457 \u0442\u0438
    \u0431 \u043E\u0431\u2019\u0454\u0434\u043D\u0430\u0432\u0441\u044F(\u043B\u0430\u0441\u044F)?"
  - "\u042F\u043A\u0431\u0438 \u0432\u0438 \u043C\u043E\u0433\u043B\u0438 \u0437\u0430\u0437\u0438\u0440\u043D\u0443\u0442\u0438
    \u0432 \u043C\u0430\u0439\u0431\u0443\u0442\u043D\u0454 \u0441\u0456\u043C\u2019\u0457
    \u043D\u0430 10 \u0440\u043E\u043A\u0456\u0432 \u0443\u043F\u0435\u0440\u0435\u0434,
    \u0449\u043E \u0431 \u0432\u0438 \u0445\u043E\u0442\u0456\u043B\u0438 \u043F\u043E\u0431\u0430\u0447\u0438\u0442\u0438?"
  - "\u042F\u043A\u0431\u0438 \u0432\u0438 \u043C\u043E\u0433\u043B\u0438 \u0432\u0438\u0440\u0443\u0448\u0438\u0442\u0438
    \u0432 \u0431\u0443\u0434\u044C-\u044F\u043A\u0443 \u0442\u043E\u0447\u043A\u0443
    \u0441\u0432\u0456\u0442\u0443, \u043A\u0443\u0434\u0438 \u0431 \u0432\u0438
    \u0432\u0438\u0440\u0443\u0448\u0438\u043B\u0438?"
  - "\u042F\u043A\u0438\u0439 \u043D\u0430\u0439\u0442\u0435\u043F\u043B\u0456\u0448\u0438\u0439
    \u0441\u043F\u043E\u0433\u0430\u0434 \u0456\u0437 \u0434\u0438\u0442\u0438\u043D\u0441\u0442\u0432\u0430,
    \u043F\u043E\u0432\u2019\u044F\u0437\u0430\u043D\u0438\u0439 \u0456\u0437 \u0441\u0456\u043C\u2019\u0454\u044E,
    \u0442\u0438 \u043F\u0430\u043C\u2019\u044F\u0442\u0430\u0454\u0448?"
  - "\u042F\u043A\u0431\u0438 \u043C\u0438 \u0433\u043E\u0442\u0443\u0432\u0430\u043B\u0438
    \u0440\u0430\u0437\u043E\u043C, \u0445\u0442\u043E \u0437 \u043D\u0430\u0441
    \u043F\u0435\u0440\u0448\u0438\u0439 \u0431\u0438 \u0437\u0430\u0431\u0440\u0443\u0434\u043D\u0438\u0432\u0441\u044F?"
  - "\u042F\u043A\u0456 \u0441\u043F\u0456\u043B\u044C\u043D\u0456 \u0456\u0433\u0440\u0438
    \u0454 \u0443 \u0432\u0430\u0448\u0456\u0439 \u0441\u0456\u043C\u2019\u0457?"
