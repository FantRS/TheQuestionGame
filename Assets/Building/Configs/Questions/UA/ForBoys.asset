  - "\u0429\u043E \u0434\u043B\u044F \u0442\u0435\u0431\u0435 \u0432\u0430\u0436\u043B\u0438\u0432\u0456\u0448\u0435:
    \u043C\u043E\u0440\u0430\u043B\u044C\u043D\u0456 \u043F\u0440\u0438\u043D\u0446\u0438\u043F\u0438
    \u0447\u0438 \u043F\u0440\u0430\u043A\u0442\u0438\u0447\u043D\u0430 \u0432\u0438\u0433\u043E\u0434\u0430?"
  - "\u0422\u0438 \u043B\u044E\u0431\u0438\u0448 \u0440\u0438\u0431\u043E\u043B\u043E\u0432\u043B\u044E
    \u0447\u0438 \u043F\u043E\u043B\u044E\u0432\u0430\u043D\u043D\u044F? \u042F\u043A\u0449\u043E
    \u0442\u0430\u043A, \u0442\u043E \u044F\u043A \u0447\u0430\u0441\u0442\u043E
    \u0446\u0438\u043C \u0437\u0430\u0439\u043C\u0430\u0454\u0448\u0441\u044F?"
  - "\u042F\u043A \u0442\u0438 \u0441\u0442\u0430\u0432\u0438\u0448\u0441\u044F \u0434\u043E
    \u043C\u043E\u0442\u043E\u0446\u0438\u043A\u043B\u0456\u0432? \u0425\u043E\u0442\u0456\u0432
    \u0431\u0438 \u043C\u0430\u0442\u0438 \u043E\u0434\u0438\u043D?"
  - "\u0427\u043E\u043C\u0443 \u0442\u0438 \u043D\u0430\u0434\u0430\u0454\u0448 \u043F\u0435\u0440\u0435\u0432\u0430\u0433\u0443:
    \u043D\u0430\u0441\u0442\u0456\u043B\u044C\u043D\u0456 \u0456\u0433\u0440\u0438
    \u0447\u0438 \u043A\u043E\u043C\u043F\u2019\u044E\u0442\u0435\u0440\u043D\u0456?"
  - "\u0427\u0438 \u0432\u0432\u0430\u0436\u0430\u0454\u0448 \u0442\u0438 \u043F\u043E\u0445\u043E\u0434\u0438
    \u043A\u043E\u0440\u0438\u0441\u043D\u0438\u043C \u0437\u0430\u043D\u044F\u0442\u0442\u044F\u043C?"
  - "\u0427\u0438 \u0454 \u0443 \u0442\u0435\u0431\u0435 \u0443\u043B\u044E\u0431\u043B\u0435\u043D\u0438\u0439
    \u0431\u043E\u0439\u043E\u0432\u0438\u043A? \u042F\u043A\u0438\u0439?"
  - "\u0422\u0438 \u043D\u0430\u0434\u0430\u0454\u0448 \u043F\u0435\u0440\u0435\u0432\u0430\u0433\u0443
    \u0437\u0431\u0438\u0440\u0430\u0442\u0438 \u0433\u0440\u043E\u0448\u0456 \u043D\u0430
    \u0432\u0435\u043B\u0438\u043A\u0456 \u0440\u0435\u0447\u0456 \u0447\u0438 \u043A\u0443\u043F\u0443\u0432\u0430\u0442\u0438
    \u043F\u043E \u0434\u0440\u0456\u0431\u043D\u0438\u0446\u044F\u0445?"
  - "\u042F\u043A\u0438\u0439 \u0442\u0432\u0456\u0439 \u0443\u043B\u044E\u0431\u043B\u0435\u043D\u0438\u0439
    \u0441\u043F\u043E\u0441\u0456\u0431 \u0432\u0456\u0434\u043F\u043E\u0447\u0438\u043D\u043A\u0443
    \u043F\u0456\u0441\u043B\u044F \u0432\u0430\u0436\u043A\u043E\u0433\u043E \u0434\u043D\u044F?"
  - "\u0427\u0438 \u0454 \u0443 \u0442\u0435\u0431\u0435 \u0443\u043B\u044E\u0431\u043B\u0435\u043D\u0438\u0439
    \u0431\u0430\u0440\u0431\u0435\u0440\u0448\u043E\u043F?"
  - "\u0422\u0438 \u043A\u043E\u043B\u0438-\u043D\u0435\u0431\u0443\u0434\u044C \u0445\u043E\u0434\u0438\u0432
    \u0443 \u043F\u043E\u0445\u0456\u0434 \u0456\u0437 \u0434\u0440\u0443\u0437\u044F\u043C\u0438?
    \u042F\u043A \u0446\u0435 \u0431\u0443\u043B\u043E?"
  - "\u0422\u0438 \u043D\u0430\u0434\u0430\u0454\u0448 \u043F\u0435\u0440\u0435\u0432\u0430\u0433\u0443
    \u043A\u0440\u0430\u0432\u0430\u0442\u043A\u0430\u043C \u0447\u0438 \u043C\u0435\u0442\u0435\u043B\u0438\u043A\u0430\u043C?"
  - "\u042F\u043A\u0435 \u0431\u0443\u043B\u043E \u043D\u0430\u0439\u0441\u043A\u043B\u0430\u0434\u043D\u0456\u0448\u0435
    \u0437\u0430\u0432\u0434\u0430\u043D\u043D\u044F, \u044F\u043A\u0435 \u0442\u0438
    \u0432\u0438\u0440\u0456\u0448\u0443\u0432\u0430\u0432 \u0441\u0432\u043E\u0457\u043C\u0438
    \u0440\u0443\u043A\u0430\u043C\u0438?"
  - "\u042F\u043A\u0430 \u0431\u0443\u043B\u0430 \u043D\u0430\u0439\u0437\u0430\u043F\u0430\u043C\u2019\u044F\u0442\u043E\u0432\u0443\u0432\u0430\u043D\u0456\u0448\u0430
    \u0437\u0443\u0441\u0442\u0440\u0456\u0447 \u0456\u0437 \u0434\u0440\u0443\u0437\u044F\u043C\u0438?"
  - "\u042F\u043A\u0438\u0439 \u0443 \u0442\u0435\u0431\u0435 \u0431\u0443\u0432
    \u043F\u0435\u0440\u0448\u0438\u0439 \u0434\u043E\u0441\u0432\u0456\u0434 \u0437
    \u0435\u043B\u0435\u043A\u0442\u0440\u043E\u0456\u043D\u0441\u0442\u0440\u0443\u043C\u0435\u043D\u0442\u0430\u043C\u0438?"
  - "\u042F\u043A\u0443 \u043F\u043E\u0440\u0430\u0434\u0443 \u0442\u0438 \u0431
    \u0434\u0430\u0432 \u043C\u043E\u043B\u043E\u0434\u0438\u043C \u0445\u043B\u043E\u043F\u0446\u044F\u043C,
    \u044F\u043A\u0456 \u0442\u0456\u043B\u044C\u043A\u0438 \u043F\u043E\u0447\u0438\u043D\u0430\u044E\u0442\u044C
    \u0434\u043E\u0440\u043E\u0441\u043B\u0435 \u0436\u0438\u0442\u0442\u044F?"
  - "\u0422\u0438 \u043D\u0430\u0434\u0430\u0454\u0448 \u043F\u0435\u0440\u0435\u0432\u0430\u0433\u0443
    \u043A\u0430\u0432\u0456, \u0447\u0430\u044E \u0447\u0438 \u0435\u043D\u0435\u0440\u0433\u0435\u0442\u0438\u043A\u0430\u043C?
    \u0427\u043E\u043C\u0443?"
  - "\u042F\u043A\u0438\u0439 \u0441\u0442\u0438\u043B\u044C \u0441\u0442\u0440\u0438\u0436\u043A\u0438
    \u0442\u0438 \u0432\u0432\u0430\u0436\u0430\u0454\u0448 \u043D\u0430\u0439\u0437\u0440\u0443\u0447\u043D\u0456\u0448\u0438\u043C?"
  - "\u0422\u0438 \u043D\u0430\u0434\u0430\u0454\u0448 \u043F\u0435\u0440\u0435\u0432\u0430\u0433\u0443
    \u043C\u0435\u0445\u0430\u043D\u0456\u0447\u043D\u0456\u0439 \u043A\u043E\u0440\u043E\u0431\u0446\u0456
    \u043F\u0435\u0440\u0435\u0434\u0430\u0447 \u0447\u0438 \u0430\u0432\u0442\u043E\u043C\u0430\u0442\u0443?"
  - "\u0422\u0438 \u043A\u043E\u043B\u0438-\u043D\u0435\u0431\u0443\u0434\u044C \u043B\u0430\u0433\u043E\u0434\u0438\u0432
    \u043F\u043E\u0431\u0443\u0442\u043E\u0432\u0443 \u0442\u0435\u0445\u043D\u0456\u043A\u0443
